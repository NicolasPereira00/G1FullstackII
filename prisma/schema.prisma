generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Produto {
  id         Int     @id @default(autoincrement())
  nome       String
  valor      Float
  quantidade Int

  @@map("produtos")
}

model Usuario {
  id    Int    @id @default(autoincrement())
  nome  String
  senha String
  tipo  String

  @@map("usuarios")
}
